<launch>

  <remap from="/subscribe" to="/distance"/>
  <remap from="/control" to="/dist_control"/>
  <node pkg="lab1" name="pid1" type="pid.py"/>
  <!--These values have been tested and work pretty well-->
    <param name="rCtrl/P" type="double" value="0.03"/>
    <param name="rCtrl/I" type="double" value="0.0"/>
    <param name="rCtrl/D" type="double" value="0.01"/>
    <param name="rCtrl/src" type="string" value="distance"/>

  <remap from="/subscribe" to="/ang"/>
  <remap from="/control" to="/ang_control"/>
  <node pkg="lab1" name="pid2" type="pid.py"/>
    <param name="rCtrl/P" type="double" value="0.01"/>
    <param name="rCtrl/I" type="double" value="0.0"/>
    <param name="rCtrl/D" type="double" value="0.00"/>
    <param name="rCtrl/src" type="string" value="angle"/>

  <remap from="/subscribe" to="/ang_vel"/>
  <remap from="/control" to="/ang_vel_control"/>
  <node pkg="lab1" name="pid3" type="pid.py"/>
    <param name="rCtrl/P" type="double" value="0.01"/>
    <param name="rCtrl/I" type="double" value="0.0"/>
    <param name="rCtrl/D" type="double" value="0.00"/>
    <param name="rCtrl/src" type="string" value="ang_vel"/>

  <node pkg="lab1" name="controlled_drive" type="controlled_drive.py"/>
    <param name="rCtrl/dist_target" type="double" value="100"/>

  <node pkg="lab1" name="balboa_serial" type="balboa_serial.py"/>

  <node pkg="lab1" name="sum" type="sum.py"/>

  <node pkg="lab1" name="ang_vel" type="ang_to_ang_vel.py"/>

</launch>
